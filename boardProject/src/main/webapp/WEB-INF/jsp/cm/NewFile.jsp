	//통합 검색 gdm
	function unifSrchView(){
	    if(srchWorChk("unifSrch #srchWord")){
	        $("#unifSrchp").val("120000000002");
	        $("form[name=unifSrch]").attr("action","/sm/SmUnifSrch.do");
	        document.unifSrch.submit();
	        
	    }
	}
	
	function clearSrch(obj){
	    if(obj.defaultValue==obj.value){
	        obj.value="";
	    }
	}
	
	//두자 이상 특수 기호 방지
	function srchWorChk(obj){
	    var jobj = $("#"+obj);
	    if(jobj.val() != null)
	    {
	        if(jobj.val() == "검색어를 입력하세요." || trim(jobj.val())==""){
	            alert("검색어를 입력하세요.");
	            jobj.val("").focus();
	            return false;
	        }
	        if(trim(jobj.val()).length < 2){
	            var tmp = new Array(2);
	            tmp[0]="검색어";
	            tmp[1]="2";
	            alert(msgBox(tmp, '{은는} {1} 자 이상 입력해야 합니다.'));
	            jobj.val("").focus();
	            return false;
	        }
	        //& -- \' " / @ < > \ $ % ; | + .. 
	        var arrTmp1 = jobj.val().split("<").length;
	        var arrTmp2 = jobj.val().split(">").length;
	        var arrTmp3 = jobj.val().split("@").length;
	        var arrTmp4 = jobj.val().split("/").length;
	        var arrTmp5 = jobj.val().split("&").length;
	        var arrTmp6 = jobj.val().split("!").length;
	        var arrTmp7 = jobj.val().split("-").length;
	        var arrTmp8 = jobj.val().split("\'").length;
	        var arrTmp9 = jobj.val().split("\"").length;
	        var arrTmp10 = jobj.val().split("\\").length;
	        var arrTmp11 = jobj.val().split("$").length;
	        var arrTmp12 = jobj.val().split("%").length;
	        var arrTmp13 = jobj.val().split(";").length;
	        var arrTmp14 = jobj.val().split("|").length;
	        var arrTmp15 = jobj.val().split("+").length;
	        var arrTmp16 = jobj.val().split(".").length;
	        var arrTmp17 = jobj.val().split("_").length;
	        var arrTmp18 = jobj.val().split("~").length;
	        
	        var totSum = arrTmp1+arrTmp2+arrTmp3+arrTmp4+arrTmp5+arrTmp6+arrTmp7+arrTmp8+arrTmp9+arrTmp10+arrTmp11+arrTmp12+arrTmp13+arrTmp14+arrTmp15+arrTmp16+arrTmp17+arrTmp18;
	        if(totSum != 18){
	            alert(msgBox('', '& - \' " / @ < >  $ % ; | + . ! _ ~ 특수 기호는 사용하실수 없습니다.'));
	            jobj.val("").focus();
	            return false;
	        }
	        return true;
	    }
	    else
	        return false;
	}
	// end 통합검색 gdm
	
	//메인화면 url 차단 여부 확인
	function fn_searchBlckYn(searchUrl){
		var resultBlckYn = "N";
		$.ajax({
		    type : 'post',
		    data : {scrUrl : searchUrl},
		    dataType : "json",
		    async : false,
		    url : "/sm/selectBlckYn.do",
		    success : function(data, status) {
				resultBlckYn = data.rstBlckYn;		   		 	
		    },
		    error : function(request,status){
		        alert(msgBox("","조회하는데 실패하였습니다."));
		    }
		});
		return resultBlckYn
	}
	
	function goToMain(){
