<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.project.boardproject.cm.service.impl.CmDAO">
	<select id="tempsql_001" resultType="java.lang.String">
		SELECT name FROM boardproject.temptbl
	</select>
	
	<select id="kwboardInq_001" resultType="java.lang.String">
		SELECT name FROM boardproject.temptbl
	</select>
	
	<insert id="kwboardWrite_001" parameterType="boardVO">
		INSERT INTO boardproject.Board (
		boardId
		,title
		,contents
		,delYn
		,atchYn
		,readCnt
		,rgtId
		,rgtDtm)
		VALUES (
		'1'
		,#{title}
		,#{contents}
		,'N'
		,'N'
		,0
		,#{rgtId}
		,DATE_FORMAT(now(), '%Y%m%d%H%i%s')
		)
	</insert>
	
	
	
	
	
</mapper>
